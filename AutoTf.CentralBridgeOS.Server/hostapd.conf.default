interface=wlan1
driver=nl80211
ssid=DefaultSSID
hw_mode=g
channel=6
wpa=2
ignore_broadcast_ssid=1
wpa_passphrase=DefaultPass123
wpa_key_mgmt=WPA-PSK
rsn_pairwise=CCMP
ctrl_interface=/var/run/hostapd
ctrl_interface_group=0
macaddr_acl=1
wpa_strict_rekey=1
accept_mac_file=/etc/hostapd/accepted_macs.txt


ht_capab=[HT40+][SHORT-GI-20][SHORT-GI-40][TX-STBC][RX-STBC1]
ieee80211n=1
tx_queue_data3_aifs=1
tx_queue_data3_cwmin=3
tx_queue_data3_cwmax=7

rts_threshold=2347
fragm_threshold=2346